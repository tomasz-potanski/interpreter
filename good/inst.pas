/* Prezentacja:
	+ instrukcji znany z jÄ™zyka c++ np. --zmienna, ++zmienna, +=, -=, /=, *=
*/

var
	dwanascie : Integer;
	dwa : Integer;
	jeden : Integer;
	trzy : Integer;
	osiem : Integer;
	szesc : Integer;

begin

	dwanascie := 3 ;
	dwanascie *= 4 ;

	dwa := 8 ;
	dwa /= 4;

	jeden := 3;
	jeden -=2;

	trzy := 1;
	trzy +=2;

	osiem:=7;
	++osiem;

	szesc:=7;
	--szesc;


end.
